{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "section-objects/*": ["./section-objects/*"],
      "page-objects/*": ["./page-objects/*"],
      "trpc": ["../src/trpc/index.ts"]
    },
    "esModuleInterop": true,
    "jsx": "react",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cy.ts",
    "**/*.spec.ts",
    "../cypress.d.ts"
  ]
}
